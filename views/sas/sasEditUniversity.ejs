
<% include ../partials/headerNavSAS %>

    <a class="btn btn-outline-secondary" href="/sas/register">Back to Menu</a>
    <h4>Update University Name</h4>
    <form id="formAddUniv" action="/sas/university/edit/<%= univId %>?_method=PUT" method=POST>
        <div class="form-group row">
            <label for="univName" class="col-sm-2 col-form-label">University Name</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="univName" name="univName" value="<%= university.universityName %>" required onkeydown="enter_univName(event)">
            </div>
        </div>
        <div class="form-group row">
            <div class="col-sm-10">
                <button type="button" class="btn btn-primary" onclick="validateAddUniv()">Submit</button>
            </div>
        </div>
    </form>

    <% include ../partials/footer %>

