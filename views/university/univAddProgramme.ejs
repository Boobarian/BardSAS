<h4>Input Programme Data</h4>
<form action="/university/add" method="POST">
  <div class="form-group row">
    <label for="inputEmail3" class="col-sm-2 col-form-label">Programme Name</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputEmail3" name="programmeName">
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Description</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputPassword3" name="description">
    </div>
  </div>
  <div class="form-group row">
    <label for="inputPassword3" class="col-sm-2 col-form-label">Closing Date</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputPassword3" name="closingDate">
    </div>
  </div>
  <div class="form-group row">
    <div class="col-sm-10">
      <button type="submit" class="btn btn-primary">Submit Data</button>
    </div>
  </div>
</form>

